# Clawdrooms Environment Configuration
# Copy to .env and fill in your values

# =============================================================================
# ANTHROPIC API (Required for both agents)
# =============================================================================
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =============================================================================
# X (Twitter) CREDENTIALS - Developer Clawd
# =============================================================================
X_USERNAME=clawdrooms
X_PASSWORD=your_x_password_here
X_EMAIL=clawdsrooms@gmail.com

# =============================================================================
# GMAIL CREDENTIALS - Developer Clawd
# =============================================================================
GMAIL_ADDRESS=clawdsrooms@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password_here

# =============================================================================
# SOLANA WALLET - Shared between agents
# =============================================================================
SOLANA_PRIVATE_KEY=your_base58_private_key_here
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# =============================================================================
# MOLTBOOK API KEYS
# =============================================================================
MOLTBOOK_API_KEY_DEV=your_dev_agent_moltbook_key
MOLTBOOK_API_KEY_ASSISTANT=your_assistant_agent_moltbook_key

# =============================================================================
# WEBSITE
# =============================================================================
PORT=3000
DOMAIN=clawdrooms.fun
WS_PORT=3001

# =============================================================================
# AGENT CONFIGURATION
# =============================================================================
DEVELOPER_AGENT_NAME=clawd-dev
ASSISTANT_AGENT_NAME=clawd-assistant

# =============================================================================
# X COMMUNITY (if applicable)
# =============================================================================
X_COMMUNITY_ID=

# =============================================================================
# TOKEN (populated after launch)
# =============================================================================
TOKEN_MINT_ADDRESS=
TOKEN_NAME=clawdrooms
TOKEN_SYMBOL=CLAWDROOMS
